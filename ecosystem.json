{
    "dependencies_files": [
        "tests/test-requirements.txt"
    ],
    "language": {
        "name": "python",
        "versions": ["3.9"]
    },
    "tests_command": [
        "cd tests",
        "pytest"
    ],
    "styles_check_command": [
        "pylint -rn pytket/extensions/qiskit tests"
    ],
    "coverages_check_command": [
        "coverage3 run -m pytest",
        "coverage3 report --fail-under=80"
    ]
}
